#include <stdio.h>

int main() {
    long long n;
    if (scanf("%lld", &n) != 1) return 0;

    printf("[");
    int first = 1;

    // Factor out 2s
    while (n % 2 == 0) {
        if (!first) printf(", ");
        printf("2");
        first = 0;
        n /= 2;
    }

    // Factor out odd numbers
    for (long long i = 3; i * i <= n; i += 2) {
        while (n % i == 0) {
            if (!first) printf(", ");
            printf("%lld", i);
            first = 0;
            n /= i;
        }
    }

    // If remainder is a prime > 1
    if (n > 1) {
        if (!first) printf(", ");
        printf("%lld", n);
    }

    printf("]\n");
    return 0;
}
