#include <stdio.h>

long long gcd_ll(long long a, long long b) {
    while (b != 0) {
        long long r = a % b;
        a = b;
        b = r;
    }
    return a;
}

long long lcm_ll(long long a, long long b) {
    return (a / gcd_ll(a, b)) * b; // avoids overflow in intermediate step
}

int main(void) {
    long long N, M;
    if (scanf("%lld %lld", &N, &M) != 2) return 0;
    printf("%lld\n", lcm_ll(N, M));
    return 0;
}

Input: 4 6
Output: 12
